<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AffichageValeur">

    <TextView
        android:id="@+id/informationas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        android:padding="8dp"
        android:text="Vos informations " />

    <TextView
        android:id="@+id/idrecup"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="78dp"
        android:text="TextView"
        />

    <TextView
        android:id="@+id/mdprecup"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginVertical="100dp"
        android:layout_marginLeft="78dp"
        android:text="TextView"
        />



    <Button
        android:id="@+id/retouraccueil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/mdprecup"
        android:layout_centerInParent="true"
        android:text="Accueil" />


</RelativeLayout>
